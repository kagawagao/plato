{
  "name": "PLATO",
  "version": "2.0.0-rc",
  "description": ":heart: a Boilerplate for SPAs use vue, vuex, vue-router",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "gfw": "cross-env CHROMEDRIVER_CDNURL=http://npm.taobao.org/mirrors/chromedriver PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs npm install -r https://registry.npm.taobao.org",
    "start": "cross-env NODE_ENV=production DEBUG=plato* nodemon .tools/bin/start",
    "dev": "cross-env DEBUG=plato* nodemon .tools/bin/start",
    "dev:mock": "npm run dev -- --mock",
    "debug": "cross-env DEBUG=plato* nodemon --debug .tools/bin/start",
    "e2e": "cross-env NODE_ENV=test DEBUG=plato* node ./test/e2e/runner.js --env firefox",
    "e2e:dev": "cross-env NODE_ENV=test DEBUG=plato* nodemon ./test/e2e/runner.js",
    "unit": "cross-env NODE_ENV=test DEBUG=plato* karma start ./test/unit/runner.js",
    "unit:dev": "npm run unit -- --watch",
    "lint": "eslint --max-warnings 10 .",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint src/themes/**/*.css",
    "test": "npm run lint && npm run lint:css && npm run unit && npm run e2e",
    "clean": "node .tools/bin/clean",
    "compile": "cross-env NODE_ENV=production DEBUG=plato* node .tools/bin/compile",
    "build": "npm run test && npm run clean && npm run compile",
    "docs": "bash .tools/bash/docs.sh",
    "push": "bash .tools/bash/push.sh"
  },
  "dependencies": {
    "leancloud-storage": "^1.5.0",
    "lodash.isplainobject": "^4.0.6",
    "nd-promise": "^3.1.0",
    "query-string": "^4.2.2",
    "string-template": "^1.0.0",
    "vue": "^2.0.3",
    "vue-router": "^2.0.1",
    "vuex": "^2.0.0",
    "vuex-actions": "^1.1.1",
    "vuex-localstorage": "^0.2.0",
    "whatwg-fetch": "^1.0.0"
  },
