    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
